<Window x:Class="VolatilityGUI.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VolatilityGUI"
        mc:Ignorable="d"
        Title="History" WindowStartupLocation="CenterScreen" Height="600" Width="530" ResizeMode="NoResize">
    <Grid>
        <RichTextBox Name="HistoryBox" ScrollViewer.VerticalScrollBarVisibility="Visible" IsReadOnly="True"/>
    </Grid>
</Window>
